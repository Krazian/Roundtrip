<h1 class="banner">Your Trips</h1>

<h1><%= link_to "Your bookmarks", user_location_pois_path(@user_id, @location_id), class: "banner"%></h1>


<% @trips.each do |trip| %>
    <a href="/users/<%= @user_id %>/locations/<%=@location_id%>/trips/<%=trip.id%>"> 
    	<div class="trips-overview pure-u-1">
            <% trip.pois.each do |poi| %>
                <%= render partial: 'poi_trip', locals: {poi: poi} %>
            <% end %>
    	</div>
	</a>
<% end %>
