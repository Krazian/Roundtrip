<!--add dynamic location to h3 tag-->

<h3>Points of Interest in <%= @location.name %> </h3> 

<%= form_tag user_location_pois_path(@user_id, @location) do |f| %>
	<% @suggestions.each do |sugg| %>
		<%= render partial: '/pois/poi_form', locals: {poi: sugg} %>
		<% end %>
	<%= submit_tag 'Bookmark Places' %>
<% end %>
