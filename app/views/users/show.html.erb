<%= form_tag("/users/#{@user_id}/locations") do %>
    <%= label_tag(:q, "Destination:") %>
    <%= search_field_tag :q %>
    <%= submit_tag "Search" %>
  <% end %>


<% if @logged_in %>
	<% @trips.each do |trip| %>
		<%= trip.location.name %>
		<br>
	<% end %>
<% end %>