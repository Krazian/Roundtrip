<!DOCTYPE html>
<html>
    <head>
        <title>Trippy</title>
        <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
        <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
        <%= csrf_meta_tags %>
    </head>
    <body>
        <div class="container" >
            <div class = "pure-g">
                <% if session[:user_id] %>
                    <%= render partial: 'layouts/log_navbar' %>
                <% else %>
                    <%= render partial: 'layouts/logout_navbar' %>
                <% end %>
            </div>
            <%= yield %>
        </div>
    </body>
</html>
